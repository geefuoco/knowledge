    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.7",
    "@types/nodemon": "^1.19.2",
    "@types/passport": "^1.0.10",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.6.5",
    "@types/prettier": "^2.7.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
