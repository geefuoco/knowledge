{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "test": "jest --watch --runInBand",
    "get-types": "typesync && npm i",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
