    "prebuild": "cd ../client/ && npm run build && cd ../server && cp -r ../client/dist ./ && rm -rf build",
    "build": "tsc",
    "postbuild": "cp ./src/config/banned_words.txt ./build/config/ && mv ./dist ./build/dist",
    "watch": "tsc --watch",
    "start": "npm run build && NODE_ENV=production node build/server.js"
  },
